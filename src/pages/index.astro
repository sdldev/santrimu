---
import Layout from "@layouts/Layout.astro";
import Home from "@components/Home/Home.astro";

export interface Props {
  title?: string;
  description?: string;
  image?: string;
}
const {
  title = "Satrimu - Aplikasi Santri",
  description = "Satrimu - Aplikasi terpadu santri untuk pondok pesantren (Boarding School)",
  image = "/logo.png",
} = Astro.props;
---

<Layout title={title} description={description} image={image}>
  <Home />
</Layout>
